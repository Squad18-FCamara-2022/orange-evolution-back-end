{
  "compilerOptions": {
    "target": "es2017",
    "lib": [
      "es6"
    ],
    "outDir": "dist",
    "module": "commonjs",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "allowJs": true,
    "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}